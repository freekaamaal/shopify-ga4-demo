{
  "kill_switch": false,
  "defaults": {
    "preserveClickIds": true,
    "preSetCampaign": false
  },
  "rules": [
    {
      "name": "FKM: facebook → woos (sitewide)",
      "domains": ["shop.freekaamaal.com"],
      "pathIncludes": ["/"],
      "pathExcludes": ["orders","thank","receipt","confirmation"],
      "match": { "utm_source_in": ["facebook"] },
      "outcome": {
        "action": "change",
        "utm": { "source": "woos", "medium": "override", "campaign": "deal-aug" }
      }
    },
    {
      "name": "FKM: empty → woos (sitewide)",
      "domains": ["shop.freekaamaal.com"],
      "pathIncludes": ["/"],
      "pathExcludes": ["orders","thank","receipt","confirmation"],
      "match": { "utm_source_in": [""] },
      "outcome": {
        "action": "change",
        "utm": { "source": "woos", "medium": "override", "campaign": "deal-aug" }
      }
    }
  ]
}
